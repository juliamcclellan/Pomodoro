{
  "manifest_version": 2,
  "name": "Pomodoro Workout Extension: The App",
  "version": "1.0",
  "description": "A pomodoro timer that suggests workouts during your breaks",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "htmlcss/popup.html"
  },
  "permissions": ["tabs", "storage"],
  "background": {
    "scripts": ["js/block.js"],
    "persistant": false
  }
}
